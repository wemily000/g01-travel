<body class="body-2">
  <%- include("partials/navigation"); %>
  <div class="section cc-home-wrap">
    <div style="display: flex;margin-top: 5%;">
      <div class="fotorama" data-allowfullscreen="true" data-maxwidth="500" data-maxheight="500" style="margin-left: 3%;flex-grow: 3;flex-shrink: 0;"><!-- data-nav="thumbs" -->
        <% for(let i=0;i<data[0].path.length;i++){ %>
          <img src="<%= data[0].path[i] %>" alt="">
        <% } %>
      </div>
      <div style="flex-grow: 7;">
        <h1><%= data[0].name %></h1>
        <input type="button" id="addcart" style="background-image:url('/images/444.png');background-size: cover;width: 50px;height: 50px;background-color: transparent;" />
        <div class="row" style="font-size: 20px;margin-top: 3%;">
          <div class="col-md-1"><img src="/images/addricon.png" width="40px"></div>
          <div class="col-md-11"><%= data[0].address %></div>          
        </div>
        <div class="row" style="font-size: 20px;margin-top: 3%;">
          <div class="col-md-1"><img src="/images/telicon.png" width="50px"></div>
          <div class="col-md-11"><%= data[0].telphone %></div>
        </div>
        <div class="row" style="font-size: 20px;margin-top: 3%;">
          <div class="col-md-1"><img src="/images/timeicon.png" width="40px"></div>
          <div class="col-md-11"><%= data[0].opentime %></div>
        </div>
        <% if(data[0].part_name != 'spot'){ %>
        <div class="row" style="font-size: 20px;margin-top: 3%;">
          <div class="col-md-1"><img src="/images/saleicon.png" width="40px"></div>
          <div class="col-md-11"><%= data[0].price %></div>
        </div>
        <% }else{ %>
        <div class="row" style="font-size: 20px;margin-top: 3%;">
          <div class="col-md-1"><img src="/images/trafficicon.png" width="50px"></div>
          <div class="col-md-11" style="word-wrap: break-word;"><%= data[0].traffic %></div>
        </div>
        <% } %>
      </div>
    </div>
    <div class="container">
      <div class="motto-wrap"></div>
      <div class="divider"></div>
      <p class="paragraph-light"><%= data[0].detail %></p>
      <div class="about-story-wrap"></div>
      <div class="divider"></div>
    </div>
    <div>
      <button class="btn btn-outline-dark" id="Back">
        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-left-square-fill" viewBox="0 0 16 16">
        <path d="M16 14a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V2a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v12zm-4.5-6.5H5.707l2.147-2.146a.5.5 0 1 0-.708-.708l-3 3a.5.5 0 0 0 0 .708l3 3a.5.5 0 0 0 .708-.708L5.707 8.5H11.5a.5.5 0 0 0 0-1z"/>
      </svg>上一頁</button>      
    </div>
  </div>
  <div class="section">
    <div class="container"></div>
  </div>
  <div class="section-copy">
    <div class="container">
      <div class="footer-wrap">
        <a href="https://webflow.com/" target="_blank" class="webflow-link w-inline-block"><img src="images/webflow-w-small2x_1webflow-w-small2x.png" width="15" alt="" class="webflow-logo-tiny">
          <div class="paragraph-tiny"></div>
        </a>
      </div>
    </div>
  </div>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=607019f9ac1405c0494569a4" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="/js/webflow.js" type="text/javascript"></script>
  <script src="/js/fotorama.dev.js" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->
  <script>
    $("#Back").on("click",function(e){
      var href = new URL(window.location.href);      
      var params = new URLSearchParams(href.search);
      params.set('pagenum', parseInt(href.searchParams.get('detailid')/10)+1);
      params.delete('detailid');
      var newhref = new URL(href.origin);
      newhref.pathname = "/Category";
      newhref.search = params;
      window.location.href=newhref;
    });
  </script>
</body>
</html>