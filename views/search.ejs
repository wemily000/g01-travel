<body>
  <%- include("partials/navigation"); %>
  <div class="intro-header-copy-copy">
    <div class="intro-content cc-homepage"></div>
  </div>
  <section id="contact-form" class="contact-form-3">
    <div class="w-container">
      <h2>搜尋</h2>
      <div class="w-form">
        <form id="wf-form-Contact-Form" name="wf-form-Contact-Form" data-name="Contact Form">
            <div class="row" style="margin-top: 10px;">
              <div class="col-md-2"><label style="font-size: 20px;">類別</label></div>
              <div class="col-md-3">
                <select id="category" name="category" class="w-select">
                  <option value="">-請選擇-</option>
                  <% for(let i=0;i<data.category.length;i++){ %>
                    <option value="<%= data.category[i].category_id %>"><%= data.category[i].category_title %></option>    
                  <% } %>
                </select>
              </div>
            </div>
            <div class="row" style="margin-top: 10px;">
              <div class="col-md-2"><label style="font-size: 20px;">名稱</label></div>
              <div class="col-md-10"><input type="text" id="foodname" class="form-control" placeholder="請輸入詳細名稱"/></div>
            </div>
            <div class="row" style="margin-top: 10px;">
              <div class="col-md-2"><label style="font-size: 20px;">地址</label></div>
              <div class="col-md-3"><select id="addrone" name="addrone" class="w-select">
                <option value="">-請選擇-</option>
                <% for(let i=0;i<data.parentaddress.length;i++){ %>
                  <option value="<%= data.parentaddress[i].addr_code_id %>"><%= data.parentaddress[i].addr_code_name %></option>    
                <% } %>      
              </select></div>
              <div class="col-md-3"><select id="addrtwo" name="addrtwo" class="w-select">
                <option value="">-請選擇-</option>
              </select></div>
              <div class="col-md-4"><input type="text" id="adress" class="form-control"/></div>
            </div>
            <input type="button" id="searchbutton" value="search" data-wait="Please wait..." class="btn btn-primary"/>
            <input type="button" id="resetbutton" value="reset" data-wait="Please wait..." class="btn btn-primary"/>
        </form>
      </div>
    </div>
  </section>
  <div class="section cc-store-home-wrap">
    <div class="container">
      <div class="divider2"></div>
      <div class="home-content-wrap">
        <div id="data-container"></div>
        <div id="pagination-container"></div>
        </div>
      </div>
  </div>

  <div class="section-copy">
    <div class="container">
      <div class="footer-wrap">
      </div>
    </div>
  </div>
  <input type="hidden" id="allchildaddr" value="<%= JSON.stringify(data.childaddress) %>" />
  <input type="hidden" id="maincategory" value="<%= data.maincategory %>" />
  <input type="hidden" id="pagenum" value="<%= data.pagenum %>">
